#include "hw11.h"

int main(){

	LA s1(3);
	s1.setMat(0,0,1);
	s1.setMat(1,1,1);
	s1.setMat(2,2,1);

	LA s2(3);
	s2.setMat(0,0,1);
	s2.setMat(1,1,1);
	s2.setMat(2,2,1);
	for(int i=0; i < 3; i++){ for( int j=0; j < 3; j++) cout << s1(i,j) << " "; cout << endl;}
	return 0;
	/*
	double *b = new double[3];
	for(int i=0; i < 3; i++) b[i] = i;
	 */
	s1 = (s1 + s2);

	for(int i=0; i < 3; i++){ for( int j=0; j < 3; j++) cout << s1(i,j) << " "; cout << endl;}
	return 0;
}



